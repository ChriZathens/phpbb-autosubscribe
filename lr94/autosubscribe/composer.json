{
    "name": "lr94/autosubscribe",
    "type": "phpbb-extension",
    "description": "Auto subscribe",
    "version": "1.0.0-beta",
    "time": "2017-04-23",
    "keywords": ["automatic", "subscription"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "lr94",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Automatic subscription",
        "soft-require": {
            "phpbb/phpbb": "~3.2"
        },
	"version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/connection_logger",
            "filename": "version_check"
        }
    }
}
